    "es6-shim": "^0.35.4",
    "jasmine": "^3.4.0",
    "jest": "^24.9.0",
    "jest-preset-typescript": "^1.2.0",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "ts-jest": "^24.0.2",
    "tslint": "^6.1.0",
    "typescript": "^3.5.3"
